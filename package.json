{
	"name": "react-hook-clock",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.14",
		"@fortawesome/free-solid-svg-icons": "^5.7.1",
		"@fortawesome/react-fontawesome": "^0.1.4",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.23",
		"prop-types": "^15.6.2",
		"react": "^16.8.1",
		"react-dom": "^16.8.1"
	},
	"devDependencies": {
		"eslint-plugin-react-hooks": "^1.0.1",
		"gh-pages": "^2.0.1",
		"react-scripts": "2.1.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "yarn run build",
		"deploy": "gh-pages -d build"
	},
	"homepage": "http://cbrannen9a.github.io/react-hook-clock",
	"eslintConfig": {
		"extends": "react-app",
		"plugins": [
			"react-hooks"
		],
		"rules": {
			"react-hooks/rules-of-hooks": "error",
			"semi": [
				2,
				"always"
			],
			"quotes": [
				2,
				"single",
				{
					"avoidEscape": true
				}
			]
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}